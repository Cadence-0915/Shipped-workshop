<h1>Sign Up Here!</h1>

<div class="inline"><%= form_for @user, :url => users_path,
:html => { :multipart => true } do |f| %>
	<%=f.label :name %>
	<%=f.text_field :name %>

	<%=f.label :username %>
	<%=f.text_field :username %>

	<%=f.label :password %>
	<%=f.password_field :password %>
	<%=f.file_field :avatar %>
	<%=f.submit "Enter" %>
</div>
<% end %>

<h2>List your boats today!</h2>

<button id="ship"><%= link_to "Add new Boats", new_boat_path %></button>
